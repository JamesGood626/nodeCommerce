{
  "name": "nodecommerce",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "prompt": "ts-node ./prompt.ts",
    "start": "ts-node ./server.ts",
    "server": "./node_modules/nodemon/bin/nodemon.js",
    "client": "npm run start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest --runInBand --forceExit --watch",
    "lint": "tslint --project tsconfig.json --config tslint.json"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!mongoose/)"
    ]
  },
  "dependencies": {
    "apollo-server-express": "^2.0.3",
    "aws-sdk": "^2.279.1",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "connect-flash": "^0.1.1",
    "connect-redis": "^3.3.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-session": "^1.15.6",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.2",
    "jimp": "^0.2.28",
    "mongoose": "^5.0.18",
    "multer": "^1.3.1",
    "npm": "^6.4.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.3",
    "redis": "^2.8.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/connect-flash": "0.0.34",
    "@types/connect-redis": "0.0.7",
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.6.1",
    "@types/express-session": "^1.15.10",
    "@types/graphql": "^0.13.1",
    "@types/jest": "^23.1.3",
    "@types/mongoose": "^5.0.16",
    "@types/multer": "^1.3.7",
    "@types/passport": "^0.4.5",
    "@types/passport-local": "^1.0.33",
    "@types/pug": "^2.0.4",
    "@types/redis": "^2.8.6",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "jest": "^23.2.0",
    "nodemon": "^1.17.4",
    "supertest": "^3.1.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  }
}
